{
  "expo": {
    "name": "RestSmart",
    "slug": "RestSmart",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "white",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.miurayuuki.RestSmart"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/NunitoSans.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "packagerOpts": {
      "sourceExts": [
        "js",
        "jsx",
        "svgx",
        "ts",
        "tsx"
      ],
      "transformer": "node_modules/react-native-svg-transformer/index.js"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "208ea899-efea-4f9c-b626-e1032b333ee7"
      }
    }
  }
}
